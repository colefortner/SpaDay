@{
}

<h1>@ViewBag.error</h1>

<form method="post">
    <div>
        <label for="useername">Username</label>
        <input name="username" type="text" id="username" value="@ViewBag.name"/>
    </div>
    <div>
        <label for="email">Email</label>
        <input name="email" type="email" id="email" value="@ViewBag.email" />
    </div>
    <div>
        <label for="password">Password</label>
        <input name="password" type="password" id="password" />
    </div>
    <div>
        <label for="verify">Re-enter Password</label>
        <input name="verify" type="password" id="verify" />
    </div>
    <input type="submit" value="Create User" />
</form>


